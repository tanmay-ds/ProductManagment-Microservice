package com.productmanagment.productmngmt.exceptionhandler;


import com.productmanagment.productmngmt.model.ResponseModel;

public class ClientErrorException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 3054265676486160891L;

	private final ResponseModel responseModel = null;

	public ClientErrorException(ResponseModel responseModel) {
		this.responseModel = responseModel;
	}
	
	public ClientErrorException(String message) {
		super(message);
	}

	public ResponseModel getResponseModel() {
		return responseModel;
	}
}
