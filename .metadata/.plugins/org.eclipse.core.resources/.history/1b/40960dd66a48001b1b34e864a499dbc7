spring:
  application:
    name: GatewayService
  cloud:    gateway:      routes:      - id: product
        uri: lb://PRODUCT-INFO
        predicates:        - Path=/product/** 
          
      - id: user
        uri: lb://USER-INFO
        predicates:
        - Path=/users/** 
server:  port: 8989
  
eureka:
  client:
    register-with-eureka: true
    fetch-registry: true
    service-url:
      defaultZone: http://localhost:8761/eureka
  instance:
    hostname: localhost